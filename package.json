{
  "name": "code-complexity-helpers",
  "version": "1.0.1",
  "description": "Helper methods to reduce calculated code complexity and code paths to be tested for coverage.",
  "main": "dist/cjs/index.js",
  "module": "dist/index.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "files": [
    "dist", "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toadskipper/code-complexity-helpers.git"
  },
  "author": "Patrick Tafoya",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/toadskipper/code-complexity-helpers/issues"
  },
  "homepage": "https://github.com/toadskipper/code-complexity-helpers#readme",
  "dependencies": {
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "jest": "^29.4.3",
    "ts-jest": "^29.0.5"
  }
}
